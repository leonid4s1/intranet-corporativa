    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "sass-embedded": "^1.89.2",
    "start-server-and-test": "^2.0.12",
