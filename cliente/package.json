    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "pinia": "^3.0.3",
    "react-router-dom": "^7.6.2",
    "timezone": "^1.0.23",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vue-virtual-scroll-grid": "^1.11.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
